{
  "executionRoleArn": "arn:aws:iam::966104787185:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "bluetechlab-docker",
      "image": "bluetechlab",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "ecs-demo"
}